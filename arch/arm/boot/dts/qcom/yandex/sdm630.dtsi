/ {
	firmware: firmware {
		android {
			fstab {
				vendor {
					fsmgr_flags = "wait,verify,slotselect";
				};
			};
		};
	};


};


&soc {
	gpio_keys {
		/delete-node/ camera_focus;
		/delete-node/ camera_snapshot;
	};

	hall {
		compatible = "hall_sensor";
		interrupt-parent = <&tlmm>;
		interrupts = <75 0x0>;
		apx9136,irq-gpio = <&tlmm 75 0x2008>;
	};

};
